d = {
    '1': ['2', '3','4'],
    '2': ['1','4','5'],
    '3': ['1','4'],
    '4': ['2','3','7'],
    '5': ['2','6','7'],
    '6': ['5','7'],
    '7': ['4','5','6'],
    
}

visited=set()

def dfs(d,visited,node):
	if node not in visited:
		print(node)
		visited.add(node)
		for neigh in d[node]:
			dfs(d,visited,neigh)
print("DFS=>")
dfs(d,visited,'1')
