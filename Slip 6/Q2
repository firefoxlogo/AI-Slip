d = {
    '1': ['2', '3'],
    '2': ['4'],
    '3': ['2'],
    '4': ['6','5'],
    '5': ['3','7'],
    '6': ['3'],
    '7': ['6'],
    
}

visited=[]
queue=[]
def bfs(d,visited,node):
	visited.append(node)
	queue.append(node)
	while queue:
		p=queue.pop(0)
		print(p)
		for neigh in d[p]:
			if neigh not in visited:
				visited.append(neigh)
				queue.append(neigh)
print("BFS=>")
bfs(d,visited,'1')
