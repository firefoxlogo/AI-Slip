d = {
    '1': ['2', '3'],
    '2': ['4'],
    '3': ['2'],
    '4': ['6','5'],
    '5': ['3','7'],
    '6': ['3'],
    '7': ['6'],
    
}

visited=set()

def dfs(d,visited,node):
	if node not in visited:
		print(node)
		visited.add(node)
		for neigh in d[node]:
			dfs(d,visited,neigh)
print("DFS=>")
dfs(d,visited,'1')
