d = {
    '1': ['2', '3'],
    '2': ['1', '4', '5'],
    '3': ['1', '6', '7'],
    '4': ['2', '8'],
    '5': ['2', '8'],
    '6': ['3', '8'],
    '7': ['3', '8'],
    '8': ['4', '5', '6', '7']
}

visited=[]
queue=[]
def bfs(d,visited,node):
	visited.append(node)
	queue.append(node)
	while queue:
		p=queue.pop(0)
		print(p)
		for neigh in d[p]:
			if neigh not in visited:
				visited.append(neigh)
				queue.append(neigh)
print("BFS=>")
bfs(d,visited,'1')
