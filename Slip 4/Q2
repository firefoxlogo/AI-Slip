d = {
    '1': ['2', '3'],
    '2': ['1', '4', '5'],
    '3': ['1', '6', '7'],
    '4': ['2', '8'],
    '5': ['2', '8'],
    '6': ['3', '8'],
    '7': ['3', '8'],
    '8': ['4', '5', '6', '7']
}

visited=set()

def dfs(d,visited,node):
	if node not in visited:
		print(node)
		visited.add(node)
		for neigh in d[node]:
			dfs(d,visited,neigh)
print("DFS=>")
dfs(d,visited,'1')
