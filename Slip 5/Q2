d = {
    '1': ['2', '4'],
    '2': ['3'],
    '3': ['4','5','6'],
    '4': ['2'],
    '5': ['7','8'],
    '6': ['8'],
    '7': ['8'],
    '8':[]
    
}

visited=[]
queue=[]
def bfs(d,visited,node):
	visited.append(node)
	queue.append(node)
	while queue:
		p=queue.pop(0)
		print(p)
		for neigh in d[p]:
			if neigh not in visited:
				visited.append(neigh)
				queue.append(neigh)
print("BFS=>")
bfs(d,visited,'1')
