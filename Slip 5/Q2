d = {
    '1': ['2', '4'],
    '2': ['3'],
    '3': ['4','5','6'],
    '4': ['2'],
    '5': ['7','8'],
    '6': ['8'],
    '7': ['8'],
    '8':[]
    
}

visited=set()

def dfs(d,visited,node):
	if node not in visited:
		print(node)
		visited.add(node)
		for neigh in d[node]:
			dfs(d,visited,neigh)
print("DFS=>")
dfs(d,visited,'1')
